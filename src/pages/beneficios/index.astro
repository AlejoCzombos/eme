---
import Layout from "../../layouts/Layout.astro";

import { banner, SEO, membership_selector } from "../../data/benefits.json";
import SectionBanner from "../../components/services/SectionBanner.astro";
import AdvertisingBanner from "../../components/services/AdvertisingBanner.astro";
import ThreeColumnsGrid from "../../components/UI/ThreeColumnsGrid.astro";
import MembershipCard from "../../components/home/MembershipCard.astro";
import Section from "../../components/UI/Section.astro";
import Membership from "../../components/home/Membership.astro";
import MembershipSelector from "../../components/benefits/MembershipSelector.astro";
---

<Layout title={SEO.title} description={SEO.description}>
    <main class="mt-[135px] relative">
        <SectionBanner backgroundImage={banner.image} title={banner.title} subtitle={banner.subtitle} />
        <AdvertisingBanner image={{src: "", alt: ""}} />
        <MembershipSelector membership_selector={membership_selector} />
    </main>
</Layout>