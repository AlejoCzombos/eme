import ServiceList from "../../components/services/ServiceList.astro";
import AdvertisingBanner from "../../components/services/AdvertisingBanner.astro";
import FeaturesListWhite from "../../components/services/FeaturesListWhite.astro";
---

<Layout title="EME - Planes familiares" description="Para familiares y personas que buscan servicios de salud prehospitalaria confiable y accesible.">
    <main class="mt-[135px] relative">
        <SectionBanner backgroundImage={banner.image} title={banner.title} subtitle={banner.subtitle} />
        <FeaturesListWhite features={features}/>
        <ServiceList services={service_list.services}/>
        <AdvertisingBanner image={{src:"/images/publicidad.png", alt:"publicidad aplicacion EME para telÃ©fonos"}} />
    </main>
