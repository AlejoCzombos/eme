---
import data from "../../data/floating-menu.json";
const {buttons} = data
---

<div class="fixed bottom-10 right-20 z-30 flex flex-col gap-3">
    {
        buttons.map((button) => (
            <a target='_blank' href={button.url} class="rounded-full bg-secondary-800 p-2 hover:scale-105 transition-transform duration-300 ease-in-out">
                <img src={button.image.src} alt={button.image.alt} class="w-12 h-12">
            </a>
        ))
    }
</div>